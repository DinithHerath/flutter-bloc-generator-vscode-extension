{
	"name": "flutter-bloc-generator",
	"publisher": "DinithHerath",
	"displayName": "Flutter Bloc Generator",
	"description": "Flutter Bloc code generator extension",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:flutter-bloc-generator.addToModel",
		"onCommand:flutter-bloc-generator.blocGen"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "flutter-bloc-generator.addToModel",
				"title": "Flutter Bloc: Add To Model"
			},
			{
				"command": "flutter-bloc-generator.blocGen",
				"title": "Flutter Bloc: Bloc Generate"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "flutter-bloc-generator.addToModel",
					"group": "flutterbloc",
					"when": "resourceLangId == dart"
				},
				{
					"command": "flutter-bloc-generator.blocGen",
					"group": "flutterbloc",
					"when": "explorerResourceIsFolder"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"fs-extra": "^10.0.0"
	}
}
